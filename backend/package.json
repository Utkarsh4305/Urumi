{
  "name": "urumi-backend",
  "version": "1.0.0",
  "description": "Backend API for Kubernetes ecommerce store provisioning platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "knex migrate:latest --knexfile src/config/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/config/knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile src/config/knexfile.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["kubernetes", "ecommerce", "woocommerce", "provisioning"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@kubernetes/client-node": "^0.20.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
